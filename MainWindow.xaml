<Window x:Class="Week_8_Junk_Notes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week_8_Junk_Notes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas Margin="0,3,0,0" Background="#FFD81515">

        <Button Content="BtnOpenNewWindow" Click="Button_Click" Grid.Column="2" Canvas.Left="600" Canvas.Top="366" Height="66" Width="201"/>

        <ListBox x:Name="lbShowNames" Grid.Column="2" Height="216" Canvas.Left="361" Canvas.Top="48" Width="211"/>

        <TextBox x:Name="txtName" TextWrapping="Wrap" Text="TextBox" Grid.Column="2" Canvas.Left="18" Canvas.Top="54" Width="165" HorizontalAlignment="Left" VerticalAlignment="Center"/>

        <Button Content="Add Name" Click="Button_Click_1" Grid.Column="2" Canvas.Left="28" Canvas.Top="112" HorizontalAlignment="Left" VerticalAlignment="Center" Width="137"/>

        <Label x:Name="lblBuisinessName"
            Content="The Biggest Name" Canvas.Left="75" Canvas.Top="214"/>

    </Canvas>
</Window>
